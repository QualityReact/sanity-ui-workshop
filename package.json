{
  "name": "@sanity/ui-workshop",
  "version": "1.0.0-beta.3",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "module": "./dist/sanity-ui-workshop.esm.js",
  "main": "./dist/sanity-ui-workshop.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/sanity-ui-workshop.esm.js",
      "require": "./dist/sanity-ui-workshop.js",
      "default": "./dist/sanity-ui-workshop.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "build": "pkg build",
    "check:package": "pkg --strict",
    "clean": "rimraf dist",
    "commit": "cz",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint . --ext .cjs,.js,.jsx,.mjs,.ts,.tsx --quiet",
    "postbuild": "run-s check:package",
    "prebuild": "run-s clean",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "semantic-release",
    "test": "jest",
    "watch": "pkg watch"
  },
  "dependencies": {
    "axe-core": "^4.4.3",
    "lodash": "^4.17.21",
    "pako": "^2.0.4",
    "segmented-property": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^17.1.0",
    "@sanity/icons": "1.3.9-beta.1",
    "@sanity/pkg-utils": "^1.15.1",
    "@sanity/semantic-release-preset": "^2.0.0",
    "@sanity/ui": "1.0.0-beta.30",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.2",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.1.2",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-axe": "^6.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@sanity/icons": "^1.0",
    "@sanity/ui": "beta",
    "react": "^18",
    "styled-components": "^5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-io/ui-workshop.git"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/ui-workshop/issues"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/",
  "keywords": [
    "sanity",
    "ui",
    "design",
    "design system",
    "prototyping",
    "testing",
    "qa"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@7.12.2",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
